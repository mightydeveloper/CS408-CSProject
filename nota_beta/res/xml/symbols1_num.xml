<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height" >

    <Row android:keyHeight="@dimen/numkey_height">
        <Key android:codes="49" android:keyLabel="1" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyLabel="2"/>
        <Key android:codes="51" android:keyLabel="3"/>
        <Key android:codes="52" android:keyLabel="4"/>
        <Key android:codes="53" android:keyLabel="5"/>
        <Key android:codes="54" android:keyLabel="6"/>
        <Key android:codes="55" android:keyLabel="7"/>
        <Key android:codes="56" android:keyLabel="8"/>
        <Key android:codes="57" android:keyLabel="9"/>
        <Key android:codes="48" android:keyLabel="0" android:keyEdgeFlags="right"/>
    </Row>      
    
    <Row>
        <Key android:codes="33" android:keyLabel="!" android:keyEdgeFlags="left"/>
        <Key android:codes="64" android:keyLabel="\@"/>
        <Key android:codes="35" android:keyLabel="\#"/>
        <Key android:codes="36" android:keyLabel="$"/>
        <Key android:codes="37" android:keyLabel="%"/>
        <Key android:codes="94" android:keyLabel="^"/>
        <Key android:codes="38" android:keyLabel="&amp;"/>
        <Key android:codes="42" android:keyLabel="*"/>
        <Key android:codes="40" android:keyLabel="("/>
        <Key android:codes="41" android:keyLabel=")" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:keyEdgeFlags="left" android:keyLabel="~" android:codes="126"/>
		<Key android:keyLabel="+" android:codes="43"/>
		<Key android:keyLabel="-" android:codes="45"/>
		<Key android:keyLabel="×" android:codes="215"/>
		<Key android:keyLabel="♥" android:codes="9829"/>
		<Key android:keyLabel="&#9825;" android:codes="9825"/>
		<Key android:keyLabel=":" android:codes="58"/>
		<Key android:keyLabel=";" android:codes="59"/>
		<Key android:keyLabel="\'" android:codes="39"/>
		<Key android:keyEdgeFlags="right" android:keyLabel="&quot;" android:codes="34"/>
    </Row>
    
    <Row>
        <Key android:keyWidth="15%p" android:keyEdgeFlags="left" android:keyLabel="1/2" android:codes="-1" android:isSticky="true" android:isModifier="true" />
		<Key android:keyLabel="=" android:codes="61"/>
		<Key android:keyLabel="_" android:codes="95"/>
		<Key android:keyLabel="&lt;" android:codes="60"/>
		<Key android:keyLabel="&gt;" android:codes="62"/>
		<Key android:keyLabel="," android:codes="44"/>
		<Key android:keyLabel="/" android:codes="47"/>
		<Key android:keyLabel="\?" android:codes="63"/>
		<Key android:keyWidth="15%p" android:keyEdgeFlags="right" android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3" android:keyIcon="@drawable/dummy" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyIcon="@drawable/dummy" android:keyWidth="10%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
                android:keyWidth="50%p" android:isRepeatable="true"/>
        <Key android:codes="46" android:keyLabel="."
                android:keyWidth="10%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="15%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
